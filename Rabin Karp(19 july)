class Solution {
    public int repeatedStringMatch(String a, String b) {
        int minloop= b.length()/a.length();
        StringBuilder temp=new StringBuilder(a);
       for(int i=0;i<minloop+2;i++){
           if(temp.toString().contains(b)) return i+1;
           else temp.append(a);
       } 

       return -1;
    }
}
